\r\n\r\n.form-main,\r\n.utilities {\r\n  padding: 0.7rem;\r\n  background-color: white;\r\n  box-shadow: 0 0 0.5em rgba(63, 63, 63, 0.493);\r\n  border-radius: 1em;\r\n}\r\n\r\n.form-main {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.form-main form {\r\n  margin-bottom: 0.7rem;\r\n}\r\n\r\n.group,\r\n.split-data {\r\n  display: flex;\r\n}\r\n\r\n.group.column {\r\n  flex-direction: column;\r\n}\r\n\r\n.split-data {\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 0 0.2rem;\r\n}\r\n\r\n.utilities .split-data {\r\n  width: 100%;\r\n  align-items: center;\r\n}\r\n\r\n.input-group {\r\n  flex-wrap: wrap;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0 0.2rem;\r\n}\r\n\r\n.input-group + .group {\r\n  margin: 1.3rem 0;\r\n}\r\n\r\n.input-group input {\r\n  flex: 2 0 230px;\r\n  padding: 0.5em;\r\n  margin-top: 0.3em;\r\n  font-size: inherit;\r\n  background-color: #eee;\r\n  border: none;\r\n  box-shadow: 0 0 0.2em rgba(63, 63, 63, 0.877) inset;\r\n  border-radius: 0.3em;\r\n}\r\n\r\n.input-group label {\r\n  flex: 1 0 100px;\r\n}\r\n\r\ninput[type=\"range\"] {\r\n  accent-color: var(--primary-clr);\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.divider {\r\n  width: 95%;\r\n}\r\n\r\n/* Credits Styling */\r\n.credits {\r\n  position: fixed;\r\n  right: -5rem;\r\n  bottom: 2rem;\r\n  padding: 1rem 1.5rem;\r\n  color: white;\r\n  background-color: rgb(33, 170, 33);\r\n  border-radius: 0.3em 0 0 0.3em;\r\n  box-shadow: 0 0 0.2em rgba(63, 63, 63, 0.411);\r\n  opacity: 0.3;\r\n  animation: slide-in-right 0.35s ease-in forwards;\r\n}\r\n\r\n.credits p {\r\n  margin: 0;\r\n}\r\n\r\n@keyframes slide-in-right {\r\n  100% {\r\n    opacity: 1;\r\n    right: 0;\r\n  }\r\n}\r\n\r\n/* ALERT STYLING */\r\n.alert {\r\n  text-transform: capitalize;\r\n  border-radius: 0.5em;\r\n  color: white;\r\n  background-color: var(--background);\r\n  padding: 1.5rem;\r\n  position: fixed;\r\n  bottom: 2rem;\r\n  left: 2rem;\r\n  transform: translateX(-2rem);\r\n  transition: 0.3s ease-in;\r\n  opacity: 0.5;\r\n  overflow: hidden;\r\n  visibility: hidden;\r\n}\r\n\r\n.alert-content {\r\n  margin: 0.3em 0.5em;\r\n}\r\n\r\n.alert::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 5px;\r\n  background-color: var(--background);\r\n  filter: brightness(50%);\r\n}\r\n\r\n.error {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n  visibility: visible;\r\n  --background: #e52b50;\r\n}\r\n\r\n.success {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n  visibility: visible;\r\n  --background: rgb(33, 170, 33);\r\n}\r\n\r\n.close-alert {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  cursor: pointer;\r\n  transition: transform 0.35s ease;\r\n}\r\n\r\n.close-alert:hover {\r\n  transform: rotate(45deg);\r\n}\r\n"},"lineCount":238}},"error":null,"hash":"9b2071215faa115a0a868ef69fa71aa4","cacheData":{"env":{}}}